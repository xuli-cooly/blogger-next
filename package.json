{
  "name": "blogger",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --ext \".js,.ts,.tsx\" .",
    "lint:style": "stylelint \"**/*.{css,scss}\"",
    "fix": "npm run fix:es && npm run fix:style && npm run fix:prettier || exit 1",
    "fix:es": "eslint --ext \".js,.tsx,.ts\" ./ --cache --fix",
    "fix:style": "stylelint \"**/*.{css,scss}\" --cache --fix ",
    "fix:prettier": "prettier --write \"**/*.{ts,tsx,js,scss,css,json,yml,yaml,md,mdx}\" "
  },
  "dependencies": {
    "next": "12.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.7.14",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5",
    "prettier": "^2.7.1",
    "typescript": "4.8.2",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^28.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-simple-import-sort": "^7.0.0"
  },
  "engines": {
    "node": ">=16.9.0"
  }
}
